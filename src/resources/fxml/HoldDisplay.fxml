<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <VBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Hold" BorderPane.alignment="CENTER">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
               <BorderPane.margin>
                  <Insets top="10.0" />
               </BorderPane.margin>
            </Label>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Label text="Angel ID: ">
                     <font>
                        <Font size="22.0" />
                     </font>
                  </Label>
                  <Label fx:id="angelIDLabel" text="null">
                     <font>
                        <Font name="System Bold" size="22.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </children>
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </VBox>
   </top>
   <center>
      <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="150.0" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0">
               <children>
                  <Label text="CLOTHES" underline="true">
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
                  <GridPane alignment="CENTER" hgap="20.0" vgap="10.0">
                    <columnConstraints>
                      <ColumnConstraints />
                      <ColumnConstraints />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <CheckBox fx:id="shirtCheckbox" mnemonicParsing="false" text="SHIRT">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </CheckBox>
                        <CheckBox fx:id="pantCheckbox" mnemonicParsing="false" text="PANT" GridPane.columnIndex="1">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </CheckBox>
                        <CheckBox fx:id="underwearCheckbox" mnemonicParsing="false" text="UNDERWEAR" GridPane.rowIndex="1">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </CheckBox>
                     </children>
                     <VBox.margin>
                        <Insets left="20.0" />
                     </VBox.margin>
                  </GridPane>
                  <Label text="SHOES" underline="true">
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
                  <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" spacing="20.0">
                     <children>
                        <CheckBox fx:id="shoeCheckbox" mnemonicParsing="false" text="SHOES">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </CheckBox>
                        <CheckBox fx:id="sockCheckbox" mnemonicParsing="false" text="SOCKS">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </CheckBox>
                     </children>
                     <VBox.margin>
                        <Insets left="20.0" />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
            <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" HBox.hgrow="NEVER">
               <children>
                  <Label text="Missing Items" underline="true">
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
                  <TableView editable="true">
                     <columns>
                        <TableColumn editable="false" prefWidth="75.0" text="Missing Items" />
                     </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </VBox>
         </children>
      </HBox>
   </center>
   <bottom>
      <Button fx:id="putOnHoldButton" mnemonicParsing="false" onAction="#onHoldButtonHandler" text="PUT ON HOLD" BorderPane.alignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
</BorderPane>
